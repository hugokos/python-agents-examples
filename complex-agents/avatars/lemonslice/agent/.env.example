LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
LIVEKIT_URL=wss://your-project.livekit.cloud

LEMONSLICE_API_KEY=your_lemonslice_api_key

# LemonSlice Avatar Toggle
# Set to "false" to disable avatar (audio-only mode for debugging)
# Set to "true" to enable avatar with video
ENABLE_LEMONSLICE_AVATAR=true

# LemonSlice Agent IDs (use dashboard-configured agents)
# These reference agents you've created in your LemonSlice dashboard
SCENARIO_1_AGENT_ID=agent_f8b4dddd8bee7a8a
# SCENARIO_2_AGENT_ID=agent_your_second_agent_id
# SCENARIO_3_AGENT_ID=agent_your_third_agent_id

# Post-Scenario Scoring Configuration
OPENAI_API_KEY=your_openai_api_key

# Storage configuration for transcripts and reports
STORAGE_TYPE=filesystem  # Options: filesystem, s3, r2
STORAGE_PATH=./data  # Local path for filesystem storage
# S3_BUCKET=your-bucket-name  # For S3/R2 storage
# S3_REGION=us-east-1
# S3_ACCESS_KEY=your_access_key
# S3_SECRET_KEY=your_secret_key